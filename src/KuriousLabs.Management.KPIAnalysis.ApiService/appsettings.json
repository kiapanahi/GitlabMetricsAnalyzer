{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "GitLab": {
    "BaseUrl": "https://gitlab.qcluster.org/"
  },
  "Jira": {
    "BaseUrl": "https://jira.tomanpay.net/"
  },
  "CrossSystem": {
    "JiraProjectKeyPattern": "[A-Z][A-Z0-9]+",
    "EnableEmailFallback": true,
    "IdentityMappings": {
      "example.gitlab.user": "jira-account-id-123",
      "another.user@company.com": "jira-account-id-456"
    }
  },
  "Metrics": {
    "Identity": {
      "BotRegexPatterns": [
        "^.*bot$",
        "^.*\\[bot\\]$",
        "^gitlab-ci$",
        "^dependabot.*",
        "^renovate.*",
        "^.*automation.*$"
      ]
    },
    "Excludes": {
      "CommitPatterns": [
        "^Merge branch.*",
        "^Merge pull request.*",
        "^Merge.*",
        "^Revert.*"
      ],
      "BranchPatterns": [
        "^dependabot/.*",
        "^renovate/.*"
      ],
      "FilePatterns": [
        "^.*\\.min\\.(js|css)$",
        "^.*\\.(png|jpg|jpeg|gif|svg|ico)$",
        "^.*\\.lock$",
        "^package-lock\\.json$",
        "^yarn\\.lock$"
      ]
    }
  }
}
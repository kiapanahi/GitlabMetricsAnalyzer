{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "GitLab": {
    "BaseUrl": "https://gitlab.example.com",
    "Token": "your-development-gitlab-token-here",
    "RootGroups": [
      "toman/core"
    ]
  },
  "Processing": {
    "MaxDegreeOfParallelism": 2,
    "BackfillDays": 30
  },
  "Exports": {
    "Directory": "./exports"
  }
}
